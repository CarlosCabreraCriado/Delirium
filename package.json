{
  "name": "Delirium",
  "main": "main.js",
  "version": "0.4.0",
  "description": "Juego de mesa RPG",
  "author": "Carlos Cabrera",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration=electron-dev",
    "start-prod": "ng serve --configuration=electron-prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "run-script-os",
    "electron:win32": "SET NODE_ENV=dev&& electron .",
    "electron:darwin": "NODE_ENV=dev electron .",
    "dist": "electron-builder",
    "dist-mac": "electron-packager . --platform=darwin",
    "electron-build": "ng build --configuration production && electron .",
    "electron-test": "ng serve && electron .",
    "dev": "NODE_ENV=localDev electron.",
    "production": "NODE_ENV=production electron.",
    "build": "NODE_ENV=production electron-builder build --win --mac --publish never",
    "deploy": "NODE_ENV=production electron-builder build --win --mac --publish always"
  },
  "dependencies": {
    "@angular/animations": "^17.0.7",
    "@angular/cdk": "^17.0.4",
    "@angular/common": "^17.0.7",
    "@angular/compiler": "^17.0.7",
    "@angular/core": "^17.0.7",
    "@angular/forms": "^17.0.7",
    "@angular/material": "^17.0.4",
    "@angular/platform-browser": "^17.0.7",
    "@angular/platform-browser-dynamic": "^17.0.7",
    "@angular/pwa": "^0.5.3",
    "@angular/router": "^17.0.7",
    "@awesome-cordova-plugins/screen-orientation": "^6.3.0",
    "@capacitor-community/electron": "^4.1.2",
    "@capacitor/android": "^5.0.0",
    "@capacitor/angular": "^2.0.3",
    "@capacitor/app": "^5.0.0",
    "@capacitor/core": "^5.0.0",
    "@capacitor/haptics": "^5.0.0",
    "@capacitor/ios": "^5.0.0",
    "@capacitor/keyboard": "^5.0.0",
    "@capacitor/screen-orientation": "^5.0.0",
    "@capacitor/status-bar": "^5.0.0",
    "@ionic/angular": "^7.0.0",
    "@ionic/storage-angular": "^4.0.0",
    "@types/nedb": "^1.8.9",
    "@types/socket.io-client": "^1.4.32",
    "animate.css": "^4.1.1",
    "convert-excel-to-json": "^1.4.1",
    "cordova-plugin-screen-orientation": "^3.0.3",
    "core-js": "^2.6.9",
    "file-system": "^1.2.2",
    "fs": "0.0.1-security",
    "ionicons": "^7.0.0",
    "jsoneditor": "^9.1.4",
    "lodash": "^4.17.21",
    "mongoose": "^5.13.4",
    "nedb": "^1.8.0",
    "ngx-json-viewer": "^2.4.0",
    "ngx-socket-io": "4.6.1",
    "node-sass": "^7.0.3",
    "path": "^0.12.7",
    "rxjs": "^7.4.0",
    "socket.io-client": "^2.3.0",
    "xlsx": "^0.17.0",
    "zone.js": "~0.14.2"
  },
  "private": true,
  "devDependencies": {
    "@angular-builders/custom-webpack": "^17.0.0",
    "@angular-devkit/architect": "^0.1700.7",
    "@angular-devkit/build-angular": "^17.0.7",
    "@angular-devkit/core": "^17.0.7",
    "@angular-devkit/schematics": "^17.0.7",
    "@angular/cli": "^17.0.7",
    "@angular/compiler-cli": "^17.0.7",
    "@angular/language-service": "^17.0.7",
    "@capacitor/assets": "^3.0.1",
    "@capacitor/cli": "^5.0.0",
    "@ionic/angular-toolkit": "^9.0.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^20.10.5",
    "codelyzer": "^6.0.0",
    "electron": "21.0.1",
    "electron-builder": "^23.6.0",
    "electron-packager": "^17.0.0",
    "jasmine-core": "4.4.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "run-script-os": "^1.1.6",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "^5.2.2"
  },
  "build": {
    "appId": "Delirium",
    "mac": {
      "category": "Juego.RPG",
      "publish": {
        "provider": "s3",
        "bucket": "carloscabreracriado"
      }
    },
    "directories": {
      "app": "./"
    }
  },
  "files": [
    "*.js",
    "delirium",
    "build",
    "node_modules"
  ],
  "postinstall": "electron-builder install-app-deps",
  "compilerOptions": {
    "typeRoots": [
      "node_modules/@types",
      "src/types"
    ]
  }
}
